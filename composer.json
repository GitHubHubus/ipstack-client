{
    "name": "ok/ipstack-client",
    "type": "library",
    "description": "A PHP wrapper for using Ipstack API",
    "keywords": ["api", "client", "ip", "ipstack", "geocode", "php"],
    "homepage": "https://github.com/GitHubHubus/ipstack-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Kochetkov",
            "email": "oleg.kochetkov999@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9"
    },
    "autoload": {
        "psr-0": {
            "OK\\Ipstack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
